{
  "name": "miza-frontend-web",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.12.0",
    "google-protobuf": "^3.12.0-rc.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/google-protobuf": "^3.7.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@yarnpkg/pnpify": "^2.0.0-rc.22",
    "parcel": "nightly",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "generate": "mkdir -p ./api/generated/ && protoc --plugin=\"protoc-gen-ts=$(which protoc-gen-ts)\" --js_out='import_style=commonjs,binary:./api/generated' --ts_out='service=grpc-web:./api/generated' ./api/todos.proto",
    "start": "parcel index.html"
  }
}
